{
  "name": "Simple Sidebar Extension",
  "version": "1.0",
  "manifest_version": 3,
  "description": "A simple sidebar extension example",
  "action": {
    "default_title": "Open Sidebar"
  },
  "permissions": ["sidePanel"],
  "background": {
    "service_worker": "background.js"
  },
  "side_panel": {
    "default_path": "sidebar.html"
  }
} 